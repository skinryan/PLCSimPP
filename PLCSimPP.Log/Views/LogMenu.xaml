<MenuItem x:Class="PLCSimPP.Log.Views.LogMenu"
          xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
          xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
          xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
          xmlns:local="clr-namespace:PLCSimPP.Log.Views"
          xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
          xmlns:prism="http://prismlibrary.com/"
          d:DesignHeight="25"
          d:DesignWidth="200"
          prism:ViewModelLocator.AutoWireViewModel="True"
          Header="Log"
          mc:Ignorable="d">

    <MenuItem Command="{Binding NavigateCommand}"
              CommandParameter="LogViewer"
              Header="Communication Log" />
    <MenuItem Command="{Binding NavigateCommand}"
              CommandParameter="Monitor"
              Header="Monitor" />
    <MenuItem Command="{Binding ExportCommand}" Header="Export" />
</MenuItem>
